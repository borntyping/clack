{
    "default": {
        "command": "fpm",
        "options": {
            "--source": "python",
            "--target": "rpm",
            "--vendor": "Sam Clements",
            "--epoch": "0"
        },
    },
    "environment": {
        "command": "scl",
        "arguments": ["enable", "python27"]
    },
    "iterations": [
        {
            "arguments": ["riemann-client"],
            "options": {
                "--version": "5.0.0",
                "--interation": "1"
            }
        },
        {
            "arguments": ["supermann"],
            "options": {
                "--version": "3.0.0",
                "--interation": "1"
            }
        }
    ]
}
